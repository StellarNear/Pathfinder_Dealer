<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="stellarnear.stellarnear">

    <application
        android:allowBackup="true"
        android:icon="@drawable/wedge_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        >
        <activity
            android:name="stellarnear.wedge_dealer.MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="stellarnear.wedge_dealer.SettingsActivity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_settings"></activity>
        <activity android:label="@string/title_det_dmg" android:name="stellarnear.wedge_dealer.DisplayDamageDetail"/>
    </application>

</manifest>